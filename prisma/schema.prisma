generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model TranslatedAlert {
  id           Int      @id @default(autoincrement())
  createdAt    DateTime @default(now())
  title        String   
  alertId      String   @unique
  summary      String
  solution     String
}

model Whitelist {
  id           Int      @id @default(autoincrement())
  createdAt    DateTime @default(now())
  url          String   @unique
  description  String
}